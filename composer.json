{
    "name":"farojas85/checkrole",
    "type":"library",
    "description":"A API Library  fro Laravel",
    "keywords":[
       "Laravel",
       "Library",
       "client"
    ],
    "license":"MIT",
    "authors":[
       {
          "name":"Fredy Rojas",
          "email":"farojas851@gmail.com",
          "role":"Developer"
       }
    ],
    "support":{
       "source":"https://github.com/farojas85/checkrole",
       "issues":"https://github.com/farojas85/checkrole/issues"
    },
    "require":{
      "php": ">=5.6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload":{
       "psr-4":{
          "Farojas85\\checkrole\\":"src"
       }
    },
    "extra": {
      "branch-alias": {
          "dev-master": "4.0-dev"
      },
      "laravel": {
          "providers": [
              "Farojas85\\chekcrole\\CheckroleServiceProvider"
          ],
          "aliases": {
              "Checkrole": "Farojas85\\checkrole\\Facades\\Checkrole"
          }
      }
  },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Farojas85\\checkrole\\test":"test"
        }
    }
 }